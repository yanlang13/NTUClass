<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Large Text"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <Button
        android:id="@+id/TravelList_shareMapButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="shareMap"
        android:text="@string/TravelList_shareMapButton" />

    <Button
        android:id="@+id/TravelLit_LinktoWebsite"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="LinktoWebsite"
        android:text="@string/TravelList_LinktoWebButton" />

</LinearLayout>
